<?xml version="1.0"?>
<!--
czas trwania schematu: 12h = 3600

pomiędzy każdą parą węzłów generowane jest 200 samochodów
z rozkładem jednostajnym
-->
<traffic>

  <scheme count="4000">
    <gateway id="L">
      <uniform a="0" b="7200"/>
    </gateway>
    <gateway id="R"/>
  </scheme>

  <scheme count="4000">
    <gateway id="R">
      <uniform a="0" b="7200"/>
    </gateway>
    <gateway id="L"/>
  </scheme>
</traffic>
